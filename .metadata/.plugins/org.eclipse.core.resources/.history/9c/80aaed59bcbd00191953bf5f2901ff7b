package com.example;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		String[] vect = sc.nextLine().split(" ");
		int position = sc.nextInt();	
		
		
		try {
			System.out.println(vect[position]);
		} catch(Exception e) {
			e.printStackTrace();
		}
	}

}
;